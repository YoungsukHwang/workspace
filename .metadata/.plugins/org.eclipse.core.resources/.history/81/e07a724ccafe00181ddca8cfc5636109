
public class Edge {
	public Node from;
	public Node dest;
	public int weight;
	
	public Node edgeFrom;
	
	public Edge(Node From, Node destination, int d) {
		this.from = From;
		this.dest = destination;
		this.weight = d;
	}
	
	public Edge(Node destination, int d) {
		this.dest = destination;
		this.weight = d;
	}

	public double getWeight() {
		return weight;
	}

	public void setEdgeFrom(Node from) {
		this.edgeFrom = from;
	}
	

}
